import dht
import machine
import network
import time

SSID= "ESP_CA"
PASSWORD= "12345678"

ap = network.WLAN(network.AP_IF)
ap.active(True)
ap.config(essid=SSID, password= PASSWORD, 
authmode = network.AUTH_WPA_WPA2_PSK)
ip_address = ap.ifconfig()[0]
print("ESP32 configurado con access point")
print("Direccion IP del Access Point: ", ip_address)
